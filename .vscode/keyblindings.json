[
  {
    "_c1":"// + Zoom na fonte",
    "key": "ctrl+alt+cmd+=",
    "command": "editor.action.fontZoomIn"
  },
  {
    "_c2":"// - Zoom na fonte",
    "key": "ctrl+alt+cmd+-",
    "command": "editor.action.fontZoomOut"
  },
  {
    "_c3":"// Reset zoom da fonte",
    "key": "ctrl+alt+cmd+0",
    "command": "editor.action.fontZoomReset"
  },
  {
    "_c4":"// Duplica linhas",
    "key": "alt+d",
    "command": "editor.action.copyLinesDownAction",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "_c5":"// Duplica linha acima",
    "key": "alt+s",
    "command": "editor.action.copyLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "_c6":"// Comenta",
    "key": "ctrl+numpad_divide",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "_c7":"// Remove o comenta original",
    "key": "ctrl+/",
    "command": "-editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+k",
    "command": "code-runner.runCustomCommand"
  },
  {
    "key": "ctrl+alt+k",
    "command": "-code-runner.runCustomCommand"
  },
  {
    "_c8":"// Salva todos os arquivos",
    "key": "ctrl+alt+cmd+s",
    "command": "workbench.action.files.saveAll"
  },
  {
    "_c9":"// Mostra o terminal",
    "key": "ctrl+'",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+shift+[BracketLeft]",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+'",
    "command": "workbench.action.terminal.focus",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+'",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "_c10":"// Mata o terminal",
    "key": "ctrl+alt+b",
    "command": "workbench.action.terminal.kill"
  },
  {
    "key": "ctrl+escape",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "_c11":"// Muda para lowercase",
    "key": "shift+alt+l",
    "command": "editor.action.transformToLowercase"
  },
  {
    "_c12":"// Muda para snake case",
    "key": "shift+alt+s",
    "command": "editor.action.transformToSnakecase"
  },
  {
    "_c13":"// Muda para title case",
    "key": "shift+alt+t",
    "command": "editor.action.transformToTitlecase"
  },
  {
    "_c14":"// Muda para uppercase",
    "key": "shift+alt+u",
    "command": "editor.action.transformToUppercase"
  },
  {
    "_c15":"// Move painel para a direita",
    "key": "ctrl+alt+3",
    "command": "workbench.action.positionPanelRight"
  },
  {
    "_c16":"// Move o painel para baixo",
    "key": "ctrl+alt+2",
    "command": "workbench.action.positionPanelBottom"
  },
  {
    "_c17":"// Abre o terminal nativo",
    "key": "ctrl+cmd+c",
    "command": "workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus"
  },
  {
    "_c18":"// Remove Abre o terminal nativo padrão",
    "key": "shift+cmd+c",
    "command": "-workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus"
  },
  {
    "_c19":"// Volta o zoom ao padrão",
    "key": "ctrl+cmd+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "_c20":"// Remove zoom padrão",
    "key": "cmd+numpad0",
    "command": "-workbench.action.zoomReset"
  },
  {
    "_Zoom in":"Zoom in",
    "key": "ctrl+cmd+=",
    "command": "workbench.action.zoomIn"
  },
  {
    "_c21":"// Remove zoom in padrão",
    "key": "cmd+numpad_add",
    "command": "-workbench.action.zoomIn"
  },
  {
    "_c22":"// Zoom out",
    "key": "ctrl+cmd+-",
    "command": "workbench.action.zoomOut"
  },
  {
    "_c23":"// Remove zoom out padrão",
    "key": "cmd+numpad_subtract",
    "command": "-workbench.action.zoomOut"
  },
  {
    "_c24":"// Novo arquivo",
    "key": "alt+cmd+n",
    "command": "explorer.newFile"
  }
]